[project]
	state = active
[access "refs/*"]
	owner = group Administrators
[access "refs/heads/*"]
	create = group nookiedevs
	push = group nookiedevs
	label-Code-Review = -2..+2 group nookiedevs
	label-Verified = -1..+1 group nookiedevs
	submit = group nookiedevs
	forgeAuthor = group nookiedevs
	forgeCommitter = group nookiedevs
